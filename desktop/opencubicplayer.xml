<?xml version="1.0" encoding="UTF-8"?>
<mime-info xmlns="http://www.freedesktop.org/standards/shared-mime-info">
 <mime-type type="audio/x-ay">
   <comment>ZX Spectrum audio file (Z80 executable code + AY-3-8910 PSG)</comment>
   <magic priority="50">
    <match type="string" value="ZXAYEMUL" offset="0"/>
   </magic>
   <glob pattern="*.ay"/>
 </mime-type>

 <mime-type type="audio/x-ams">
  <comment>Extreme Tracker / Velvet Studio</comment>
  <magic priority="50">
   <match type="string" value="Extreme" offset="0"/>
   <match type="string" value="AMShdr\032" offset="0"/>
  </magic>
  <glob pattern="*.ams"/>
 </mime-type>

 <mime-type type="audio/x-dmf">
  <comment>Delusion/XTracker Digital Music File</comment>
  <magic priority="40">
   <match type="string" value="DDMF" offset="0"/>
  </magic>
  <glob pattern="*.dmf"/>
 </mime-type>

 <mime-type type="audio/x-gbs">
  <comment>GameBoy Sound System</comment>
  <magic priority="50">
   <match type="string" value="GBS" offset="0"/>
  </magic>
